            {
              "code" : "my-authentication-login",
              "type" : "codeVoice"
            },
            {
              "text" : " and ",
              "type" : "text"
            },
            {
              "code" : "my-authentication-password",
              "type" : "codeVoice"
            },
            {
              "text" : " with the credentials provided to you:",
