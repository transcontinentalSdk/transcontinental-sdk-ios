            },
            {
              "code" : "netrc",
              "type" : "codeVoice"
            },
            {
              "text" : " internally to download private binaries.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\/\/ Package.swift",
            "\/\/ https:\/\/github.com\/transcontinentalSdk\/tcdigitalflyer-ios-agent-spm\/blob\/1.9.9\/Package.swift",
            "...",
            "targets: [",
            "    .binaryTarget(",
            "        name: \"TCDigitalFlyer\",",
            "        url:\"https:\/\/transfer.tds.transcontinental.ca\/TCDigitalFlyer-1.9.9-14.2.xcframework.zip\",",
