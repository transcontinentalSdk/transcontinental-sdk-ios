            {
              "text" : "Save your authentication details in a ",
              "type" : "text"
            },
            {
              "code" : ".netrc",
              "type" : "codeVoice"
            },
            {
              "text" : " file located in your home directory (",
              "type" : "text"
            },
            {
              "code" : "~\/.netrc",
              "type" : "codeVoice"
            },
            {
              "text" : "). Replace ",
              "type" : "text"
